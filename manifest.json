{
  "manifest_version": 2,

  "name": "today",
  "description": "the beautiful new tab view you need.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "unlimitedStorage"
  ]
}
